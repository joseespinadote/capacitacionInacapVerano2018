<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alumnossur.martes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityInvitation"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityCofee"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Main2Activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityTea"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DetalleTe"
            android:label="@string/title_activity_detalle_te"
            android:theme="@style/AppTheme"
            android:parentActivityName=".ActivityTea" />
    </application>

</manifest>